+<link type="text/css" rel = "stylesheet" href="${pageContext.request.contextPath}/resources/css/generic-page.css">
+<body>
+<div id="pageheader">
+    <!--TODO Это подлежит рефакторингу, когда сделаю соотв.класс-->
