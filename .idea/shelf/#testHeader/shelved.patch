+            <li>Главная</li>
+            <li>Доставка и оплата</li>
+            <li>Контакты</li>
+            <li>Регистрация/Вход</li>
+        </ul>
+    </div>
+</div>
+<div id="body">
+    <jsp:doBody/>
+</div>
+<div id="pagefooter">
+    <jsp:invoke fragment="footer"/>
+</div>
+</body>
+</html>
